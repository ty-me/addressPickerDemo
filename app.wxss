/**app.wxss**/
@import 'style/weui.wxss';

page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    position:relative;
    height:100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.page__hd{
  padding:18px  0;
  text-align:center;
}

.page__ft{
  padding:20px 0;
  text-align:center;
}
